#!/bin/bash

 # TODO: PageRank, CNN?
files=( "DotProduct" "OuterProduct" "TPCHQ6" "BlackScholes" "MatMult_outer" "MatMult_inner" "GDA" "LogReg" "SGD" "Kmeans" "SMV" "BFS")
#files=( "MatMult_outer" "LogReg" "SGD" "Kmeans" "SMV" "BFS")
